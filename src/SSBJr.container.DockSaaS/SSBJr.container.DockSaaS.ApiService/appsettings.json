{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Database=docksaasdb;Username=postgres;Password=postgres;",
    "Redis": "localhost:6379"
  },
  "JwtSettings": {
    "Secret": "DockSaaS-Super-Secret-Key-That-Should-Be-Changed-In-Production-2024!",
    "Issuer": "DockSaaS",
    "Audience": "DockSaaSUsers",
    "ExpirationInMinutes": 60,
    "RefreshTokenExpirationInDays": 7
  },
  "ServiceEndpoints": {
    "BaseUrl": "https://localhost:7000"
  },
  "BlazorClientUrls": "https://localhost:7001,http://localhost:5001",
  "AllowedHosts": "*",
  "HealthChecks": {
    "UI": {
      "HealthChecksUri": "/health",
      "HealthChecksUIUri": "/health-ui",
      "ApiPath": "/health-api",
      "UIPath": "/health-ui"
    }
  },
  "Aspire": {
    "HealthChecks": {
      "Enabled": true
    }
  },
  "DatabaseInitialization": {
    "SkipOnConnectionFailure": true,
    "RetryCount": 3,
    "RetryDelay": "00:00:05"
  }
}
